{
    "manifest_version": 2,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "A Chrome Extension to summarize a web-page",
    "permissions": [],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  